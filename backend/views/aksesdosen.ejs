<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Portal Akademik 1B</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-0evHe/X+R7YkIZDRvuzKMRqM+OrBnVFBL6DOitfPri4tjfHxaWutUpFmBp4vmVor" crossorigin="anonymous">
    <script src="https://kit.fontawesome.com/9c1321c239.js" crossorigin="anonymous"></script>
  </head>
  <body>
    <%- include ('modals/headadmin') %>
    <nav>
        <div class="row no-gutters mt-5">
          <%- include ('modals/navbaradmin', {nama, NIP}) %>
            <div class="col-md-10 p-4 pt-4">
                <h3><i class="fa-solid fa-universal-access me-2"></i>Hak Akses Dosen</h3><hr>
                <div class="container">
                </div><br>
                <div class="card shadow mb-4">
                    <div class="card-body">
                        <table class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th class="text-center" scope="col">No.</th>
                                    <th scope="col">Nama Mata Kuliah</th>
                                    <th class="text-center" scope="col">Kode Mata Kuliah</th>
                                    <th class="text-center" scope="col">Jumlah SKS</th>
                                    <th class="text-center" scope="col">Lihat</th>
                                </tr>
                            </thead>
                            <tbody>
                              <% RPS.forEach((R, i) => { %>
                                <tr>
                                    <th class="text-center" scope="row"><%= i+1 %></th>
                                    <td><%= R.name %></td>
                                    <td class="text-center"><%= R.code %></td>
                                    <td class="text-center"><%= R.credit %></td>
                                    <td class="text-center"><a href="/detailAksesDosen/<%= R.id %>/<%= R.name %>"><i class="fa-solid fa-eye bg-warning p-2 text-black rounded" data-toggle="tooltip" title="Open"></i></a></td>
                                </tr>
                                <% } ) %>
                            </tbody>
                        </table>
                    </div>
                </div><br>
            </div>
        </div>
    </nav>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-pprn3073KE6tl6bjs2QrFaJGz5/SUsLqktiwsUTF55Jfv3qYSDhgCecCxMW52nD2" crossorigin="anonymous"></script>
  </body>
</html>