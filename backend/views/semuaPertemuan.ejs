<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Portal Akademik 1B</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-0evHe/X+R7YkIZDRvuzKMRqM+OrBnVFBL6DOitfPri4tjfHxaWutUpFmBp4vmVor" crossorigin="anonymous">
    <script src="https://kit.fontawesome.com/9c1321c239.js" crossorigin="anonymous"></script>
  </head>
  <body>
    <%- include ('modals/headdosen') %>
    <nav>
        <div class="row no-gutters mt-5">
            <%- include ('modals/navbardosen', {nama, NIP}) %>
            <div class="col-md-10 p-4 pt-4">
                <h3><i class="fa-solid fa-calendar-days me-2"></i>Jadwal Pertemuan yang Diampu</h3><hr>
                <div class="container"><br>
                </div>
                <div class="card shadow mb-4">
                    <div class="card-body">
                        <table class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th class="text-center" scope="col">No</th>
                                    <th class="text-center" scope="col">Minggu Pertemuan</th>
                                    <th class="text-center" scope="col">Materi</th>
                                    <th class="text-center" scope="col">Metode</th>
                                    <th class="text-center" scope="col">Pengalaman Belajar Mahasiswa</th>
                                </tr>
                            </thead>
                            <body>
                                <% pertemuan.forEach((P, i) => { %>
                                <tr>
                                    <td class="text-center" scope="col"><%= i+1 %></td>
                                    <td scope="col"><%= P.week %></td>
                                    <td scope="col"><%= P.material %></td>
                                    <td scope="col"><%= P.method %></td>
                                    <td scope="col"><%= P.student_experience %></td>
                                </tr>
                                <% } ) %>
                            </body>
                        </table>
                        <a href="/semuaRPS" class="btn btn-success" type="button">Kembali</a>
                    </div>
                </div><br>
            </div>
        </div>
    </nav>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-pprn3073KE6tl6bjs2QrFaJGz5/SUsLqktiwsUTF55Jfv3qYSDhgCecCxMW52nD2" crossorigin="anonymous"></script>
  </body>
</html>