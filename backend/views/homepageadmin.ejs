<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Portal Akademik 1B</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-0evHe/X+R7YkIZDRvuzKMRqM+OrBnVFBL6DOitfPri4tjfHxaWutUpFmBp4vmVor" crossorigin="anonymous">
    <script src="https://kit.fontawesome.com/9c1321c239.js" crossorigin="anonymous"></script>
  </head>
  <body>
    <%- include ('modals/headadmin') %>
    <nav>
          <div class="row no-gutters mt-5">
            <%- include ('modals/navbaradmin', {nama, NIP}) %>
            <div class="col-md-10 p-4 pt-4">
                <h3><i class="fa-solid fa-list me-2"></i>List RPS</h3><hr>
                <div class="container">
                    <div class="row">
                        <div class="col-2"><h6>Tahun Akademik</h6></div>
                        <div class="col-2"><h6>: 2021/2022</h6></div>
                      </div>
                    <div class="row">
                      <div class="col-2"><h6>Semester</h6></div>
                      <div class="col-3"><h6>: Genap</h6></div>
                    </div><br>
                </div>
                <form class="d-flex" role="search">
                    <input class="form-control me-2" type="search" placeholder="Masukkan Nama/Kode Mata Kuliah" aria-label="Search">
                    <button class="btn btn-outline-dark" type="submit">Cari</button>
                </form><br>
                <table class="table table-striped table-bordered table-hover">
                    <thead>
                        <tr>
                            <th class="text-center" scope="col">No.</th>
                            <th class="text-center" scope="col">Nama Mata Kuliah</th>
                            <th class="text-center" scope="col">Kode Mata Kuliah</th>
                            <th class="text-center" scope="col">SKS</th>
                            <th class="text-center" scope="col">Sem</th>
                            <th class="text-center" scope="col">Lihat</th>
                            <th class="text-center" scope="col">Cetak</th>
                            <th class="text-center" scope="col">Detail</th>
                        </tr>
                    </thead>
                    <tbody>
                      <% RPS.forEach((R, i) => { %>
                        <tr>
                            <th class="text-center" scope="row"><%= i+1 %></th>
                            <td><%= R.name %></td>
                            <td class="text-center"><%= R.code %></td>
                            <td class="text-center"><%= R.credit %></td>
                            <td class="text-center"><%= R.semester %></td>
                            <td class="text-center"><a href="/detailRPS/<%= R.id %>/<%= R.name %>"><i class="fa-solid fa-eye bg-warning p-2 text-black rounded" data-toggle="tooltip" title="Open"></i></a></td>
                            <td class="text-center"><a href="/detailRPS/<%= R.id %>/<%= R.name %>"><i class="fa-solid fa-print bg-warning p-2 text-black rounded" data-toggle="tooltip" title="Print"></i></a></td>
                            <td class="text-center">
                                <div class="dropdown">
                                <button class="btn btn-info dropdown-toggle" type="button" id="dropdownMenuButton1" data-bs-toggle="dropdown" aria-expanded="false">
                                  Detail
                                </button>
                                <ul class="dropdown-menu" aria-labelledby="dropdownMenuButton1">
                                  <li><a class="dropdown-item" href="/detailAksesDosen/<%= R.id %>/<%= R.name %>">Akses Dosen</a></li>
                                  <li><a class="dropdown-item" href="/detailCPMKdanCPL/<%= R.id %>/<%= R.name %>">CPMK-CPL</a></li>
                                  <li><a class="dropdown-item" href="persentaserps.html">Persentase RPS</a></li>
                                </ul>
                              </div>
                            </td>
                        </tr>
                        <% } ) %>
                    </tbody>
                </table>
               
            </div>
        </div>
    </nav>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-pprn3073KE6tl6bjs2QrFaJGz5/SUsLqktiwsUTF55Jfv3qYSDhgCecCxMW52nD2" crossorigin="anonymous"></script>
  </body>
</html>